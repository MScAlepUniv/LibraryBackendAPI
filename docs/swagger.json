{
    "openapi": "3.0.0",
    "info": {
        "version": "1.0.0",
        "title": "library-student Application API"
    },
    "components": {
        "securitySchemes": {
            "Bearer": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
    },
    "tags": [
        {
            "name": "mongodb"
        },
        {
            "name": "mysql"
        }
    ],
    "security": [
        {
            "Bearer": []
        }
    ],
    "paths": {
        "/mysql/auth/register": {
            "post": {
                "security": [],
                "summary": "",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "Name": {
                                        "type": "string",
                                        "example": "Basheer"
                                    },
                                    "Father_name": {
                                        "type": "string",
                                        "example": "Basheer"
                                    },
                                    "Last_name": {
                                        "type": "string",
                                        "example": "Shahrour"
                                    },
                                    "Birthdate": {
                                        "type": "string",
                                        "example": "1997/2/13"
                                    },
                                    "Address": {
                                        "type": "string",
                                        "example": "Aleppo"
                                    },
                                    "Job": {
                                        "type": "string",
                                        "example": "developer"
                                    },
                                    "Qualification": {
                                        "type": "string",
                                        "example": "Computer Eng."
                                    },
                                    "user_name": {
                                        "type": "string",
                                        "example": "basheer-shahrour"
                                    },
                                    "pass": {
                                        "type": "string",
                                        "example": "123abc"
                                    },
                                    "subscriptioncard_info": {
                                        "type": "object",
                                        "properties": {
                                            "Subscription_id": {
                                                "type": "integer",
                                                "example": 125487
                                            },
                                            "State": {
                                                "type": "string",
                                                "example": "active"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Ok"
                    },
                    "409": {
                        "description": "Visitor existe"
                    }
                },
                "tags": [
                    "mysql"
                ]
            }
        },
        "/mysql/auth/login": {
            "post": {
                "security": [],
                "summary": "",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "user_name": {
                                        "type": "string",
                                        "example": "basheer-shahrour"
                                    },
                                    "pass": {
                                        "type": "string",
                                        "example": "123abc"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Ok"
                    },
                    "404": {
                        "description": "Visitor not found"
                    }
                },
                "tags": [
                    "mysql"
                ]
            }
        }
    }
}